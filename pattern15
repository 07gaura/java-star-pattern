public class pattern15
{
    public static void main(){
        int i, j;
        for(i=1;i<=5;i++){
        System.out.print("*");
        }
        System.out.print("\n");
        for(i=1;i<=5;i++){
            for(j=1;j<=5;j++){
                if (j>1&&j<5)
                System.out.print(" ");
                else
                System.out.print("*");
            }
            System.out.print("\n");
        }
        for(i=1;i<=5;i++){
        System.out.print("*");
        }
    }
}
