public class pattern14
{
    public static void main(){
    int i,j;
    for(i=1;i<=5;i++){
    System.out.print(" *");
    }
    System.out.print("\n");
    for(i=1;i<=5;i++){
        for(j=5;j>=i;j--){
            if (j<=4&&j>i)
            System.out.print("  ");
            else
            System.out.print(" *");
        }
        System.out.print("\n");
    }
    
    }
}
